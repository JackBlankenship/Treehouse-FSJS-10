tbody#item.item
	each row in rows
		tr.patron
			td
				a(href='/Patrons/Detail/' + row.id )=  row.first_name + ' ' + row.last_name  
			td= row.address 	
			td= row.email 	
			td= row.library_id
			td= row.zip_code
			td.item-name.hidden= row.first_name + row.last_name + row.address + row.email + row.library_id + row.zip_code 